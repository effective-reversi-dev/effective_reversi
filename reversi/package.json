{
  "name": "reversi-frontend",
  "version": "0.0.1",
  "private": true,
  "description": "reversi frontend assets.",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "start": "babel-node server.js",
    "build": "NODE_ENV=production webpack -p --progress --colors --config webpack.prod.config.js --bail",
    "lint": "eslint assets",
    "coverage": "jest --coverage",
    "format": "prettier --write assets/js/**/*.js",
    "check": "prettier --check assets/js/**/*.js"
  },
  "prettier": {
    "singleQuote": true
  },
  "jest": {
    "transform": {
      ".*": "<rootDir>/node_modules/jest-css-modules"
    },
    "transformIgnorePatterns": [
      "node_modules/*"
    ],
    "modulePaths": [
      "assets",
      "assets/js",
      "assets/js/app"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "identity-obj-proxy"
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "./jest-setup.js"
    ],
    "collectCoverageFrom": [
      "assets/js/**/*.{js,jsx}"
    ],
    "coveragePathIgnorePatterns": [
      "assets/js/store.js",
      "assets/js/index.js",
      "assets/js/jquery-index.js",
      "assets/js/constants/*",
      "assets/js/pages/*",
      "assets/js/tests/*"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 10
      }
    }
  },
  "dependencies": {
    "@material-ui/core": "^3.6.2",
    "@material-ui/icons": "^3.0.1",
    "@material-ui/styles": "^3.0.0-alpha.3",
    "autoprefixer": "^9.3.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "bootstrap": "^4.1.1",
    "classnames": "^2.2.5",
    "css-loader": "^1.0.1",
    "expose-loader": "^0.7.3",
    "fetch": "1.1.0",
    "file-loader": "^2.0.0",
    "golden-layout": "^1.5.9",
    "imports-loader": "^0.8.0",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "marked": "^0.5.1",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.23",
    "popper.js": "^1.12.9",
    "postcss": "^7.0.3",
    "postcss-loader": "^2.0.6",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-cookie": "3.0.8",
    "react-dev-utils": "^8.0.0",
    "react-dom": "^16.4.0",
    "react-hot-loader": "^4.8.4",
    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "4.3.1",
    "react-testing-library": "^6.1.2",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.4",
    "redux-devtools-extension": "^2.13.8",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.16.2",
    "resolve-url-loader": "^3.0.0",
    "sass-loader": "^7.0.3",
    "script-loader": "^0.7.0",
    "shortid": "^2.2.14",
    "style-loader": "^0.23.1",
    "url-loader": "^1.0.1",
    "webpack": "^4.23.1",
    "webpack-bundle-tracker": "^0.3.0",
    "webpack-dev-server": "^3.1.4",
    "webpack-spritesmith": "^0.5.3",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "ajv": "^6.2.1",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-import-resolver-webpack": "^0.10.0",
    "eslint-plugin-import": "^2.6.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "jest-css-modules": "^1.1.0",
    "jest-dom": "^3.1.3",
    "mini-css-extract-plugin": "^0.4.1",
    "node-sass": "^4.11.0",
    "prettier": "1.16.4",
    "redux-mock-store": "^1.5.3",
    "redux-saga-test-plan": "^3.7.0",
    "webpack-cli": "^3.1.0"
  }
}
